{
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": ["import", "jsx-a11y", "react", "react-hooks"],
    "rules": {
      "strict": 0
    }
  }